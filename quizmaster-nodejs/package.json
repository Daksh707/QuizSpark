{
  "name": "quizmaster-nodejs",
  "version": "1.0.0",
  "description": "Dynamic Quiz & Exam Platform - Node.js + Express.js + EJS + PostgreSQL + Socket.IO",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "echo 'Build completed - Node.js app ready for deployment'",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha test/unit/**/*.test.js",
    "test:integration": "mocha test/integration/**/*.test.js",
    "test:load": "artillery run test/load/load-test.yml",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docker:build": "docker build -t quizmaster .",
    "docker:run": "docker run -p 5000:5000 --env-file .env quizmaster",
    "docker:compose": "docker-compose up -d",
    "deploy:aws": "./deploy/aws-deploy.sh"
  },
  "keywords": [
    "quiz",
    "education",
    "nodejs",
    "express",
    "ejs",
    "postgresql",
    "socket.io",
    "real-time",
    "aws",
    "docker"
  ],
  "author": "QuizMaster Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "socket.io": "^4.7.4",
    "pg": "^8.11.3",
    "bcrypt": "^5.1.1",
    "express-session": "^1.17.3",
    "connect-pg-simple": "^9.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "redis": "^4.6.10",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "mocha": "^10.2.0",
    "chai": "^4.3.10",
    "supertest": "^6.3.3",
    "artillery": "^2.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/quizmaster-nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/quizmaster-nodejs/issues"
  },
  "homepage": "https://github.com/yourusername/quizmaster-nodejs#readme"
}